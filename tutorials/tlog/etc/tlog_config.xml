<tlog_config>
	<level>e_tlog_debug</level>
	<vector>
		<appender_vec>
			<type>e_tlog_appender_rolling_file</type>
			<appender>
					<rolling_file>
						<file_name>logs/tlog_example.log</file_name>
						<max_file_size>1024</max_file_size>
						<max_backup_index>5</max_backup_index>
					</rolling_file>
			</appender>
		</appender_vec>
		<appender_vec>
			<type>e_tlog_appender_shm</type>
			<appender>
					<shm>
						<output_tbuskey>20001</output_tbuskey>
					</shm>
			</appender>
		</appender_vec>
	</vector>
</tlog_config>
