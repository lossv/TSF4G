<tlog_config>
	<level>e_tlog_debug</level>
	<appender_vec>
		<element>
			<type>e_tlog_appender_rolling_file</type>
			<appender>
					<rolling_file>
						<file_name>logs/tlog_example.log</file_name>
						<max_file_size>1000000</max_file_size>
						<max_backup_index>10</max_backup_index>
					</rolling_file>
			</appender>
		</element>
		<element>
			<type>e_tlog_appender_daily_directory</type>
			<appender>
					<daily_directory>
						<directory>daily_logs</directory>
						<file_name>tlog_example.log</file_name>
					</daily_directory>
			</appender>
		</element>
		<!--
		<element>
			<type>e_tlog_appender_shm</type>
			<appender>
					<shm>
						<output_tbuskey>20001</output_tbuskey>
					</shm>
			</appender>
		</element>
		-->
	</appender_vec>
</tlog_config>
