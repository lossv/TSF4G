<tconnd_config>
	<log_config>
	<!include file = "etc/tconnd_log.xml"/>
	</log_config>
	<ip>127.0.0.1</ip>
	<port>7001</port>
	<interval>2000</interval>
	<package_size>10240</package_size>
	<package_connections>10000</package_connections>
	<package_ticks_limit>5</package_ticks_limit>
	<connections>32767</connections>

	<input_tbuskey>10001</input_tbuskey>
	<output_tbuskey>10002</output_tbuskey>
	<backlog>128</backlog>
	<accept_ticks_limit>5</accept_ticks_limit>

	<defer_accept>1</defer_accept>
	<sndbuf>10000000</sndbuf>
	<rcvbuf>10000000</rcvbuf>
	<nodelay>0</nodelay>
<!-- cork可以显著减少rtt -->
	<cork>0</cork>
	<keepalive>1</keepalive>
	<keepidle>120</keepidle>
	<keepintvl>30</keepintvl>
	<keepcnt>8</keepcnt>
</tconnd_config>
